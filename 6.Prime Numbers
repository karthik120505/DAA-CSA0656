#include <stdio.h>
#include <stdbool.h>
bool isPrime(int num) {
    if (num <= 1) {
        return false; 
    }
    if (num == 2) {
        return true;
    }
    if (num % 2 == 0) {
        return false; 
    }
    for (int i = 3; i <= num / 2; i += 2) {
        if (num % i == 0) {
            return false; 
        }
    }
    return true;
}

int main() {
    int num;
    printf("Karthik_192210719\n");
    printf("Enter an integer: ");
    scanf("%d", &num);
    if (isPrime(num)) {
        printf("%d is a prime number.\n", num);
    } else {
        printf("%d is not a prime number.\n", num);
    }

    return 0;
}
